# Cloudinary Configuration
# Get these values from your Cloudinary dashboard: https://cloudinary.com/console

# Your Cloudinary cloud name (found in your dashboard)
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name_here

# Your Cloudinary API key (found in your dashboard)
CLOUDINARY_API_KEY=your_api_key_here

# Your Cloudinary API secret (found in your dashboard)
CLOUDINARY_API_SECRET=your_api_secret_here

# The folder name in your Cloudinary media library where your images are stored
CLOUDINARY_FOLDER=portfolio

# Optional: Custom domain for your Cloudinary images (if you have one)
# NEXT_PUBLIC_CLOUDINARY_DOMAIN=your-custom-domain.com
